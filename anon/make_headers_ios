cmake ../relic -G "Unix Makefiles"  \
  -DRELIC_CHECK=on -DDEBUG=on -DWITH="DV;BN;MD;FP;EP;FPX;EPX;PP;PC;CP" -DARITH=easy -DARCH=ARM -DCOLOR=off -DOPSYS=NONE -DFP_METHD="INTEG;INTEG;INTEG;MONTY;EXGCD;SLIDE" -DFPX_METHD="INTEG;INTEG;LAZYR" -DPP_METHD="LAZYR;OATEP" -DCOMP="-O3" -DBN_PRECI=256 -DBN_MAGNI=DOUBLE
[ -d ios_include ] && rm -rf ios_include
mv include ios_include
